{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "My angular seed",
  "scripts": {
    "_start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:clean": "gulp clean",
    "build:vendor": "gulp vendor",
    "build:dev": "gulp clean && gulp vendor && gulp build",
    "build:dev.watch": "gulp clean && gulp vendor && gulp",
    "build:production": "gulp clean && gulp vendor && gulp build --production && node systemjs.builder.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "@angular/upgrade": "~2.2.0",
    "angular2-in-memory-web-api": "0.0.18",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "^0.19.41",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "tslint": "^4.0.2",
    "typescript": "~2.1.4",
    "typings": "^1.3.2",
    "codelyzer": "0.0.22",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.6",
    "systemjs-builder": "^0.15.19",
    "yargs": "^4.7.1"
  }
}
